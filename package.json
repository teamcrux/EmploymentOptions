{
  "name": "team-crater-eo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"export NODE_ENV=test && env PORT=3001 nodemon ./bin/www\" \"npm run dev_client\"",
    "dev_client": "cd client && npm start",
    "start": "node ./bin/www",
    "build": "cd client && npm run build && cd .. && cp -r ./client/build/* ./public",
    "production": "concurrently \"export NODE_ENV=production && env PORT=3001 forever start -c 'npm start' ./bin/www\" \"npm run client\"",
    "client": "cd client && forever start -c \"npm start\" ./"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "brfs": "^1.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "email-validator": "^1.0.7",
    "express": "~4.15.2",
    "express-session": "^1.15.2",
    "forever": "^0.15.3",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^7.4.1",
    "morgan": "~1.8.1",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.8.0",
    "pdfkit": "^0.8.3",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "serve-favicon": "~2.4.2",
    "transform-loader": "^0.2.4"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0"
  }
}
